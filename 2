#include <iostream>
using namespace std;

int main()
{
	int x;
	cout << "Vvedite x" << endl;
	cin >> x;
	if ((x % 7) == 1){
		cout << "Ponedenlik" << endl;
	}
	if ((x % 7) == 2) {
		cout << "Vtornik" << endl;
	}
	if ((x % 7) == 3) {
		cout << "Sreda" << endl;
	}
	if ((x % 7) == 4) {
		cout << "4etverg" << endl;
	}
	if ((x % 7) == 5) {
		cout << "Pyatnica" << endl;
	}
	if ((x % 7) == 6) {
		cout << "Subbota" << endl;
	}
	if ((x % 7) == 0) {
		cout << "Voskresenie" << endl;
	}
	return 0;
}
